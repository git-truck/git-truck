{
  "name": "prototype1",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "test": "vite-jest --coverage",
    "test:watch": "vite-jest --watch",
    "dev": "vite",
    "build": "tsc && vite build",
    "parse": "cd ./../parser && npm start -- --path .. --out ./prototype1/src/data.json",
    "pd": "npm run parse && npm run dev",
    "pb": "npm run parse && npm run build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@react-aria/utils": "^3.11.2",
    "d3": "^7.3.0",
    "github-colors": "^2.2.20",
    "jest": "^27.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-use": "^17.3.2",
    "vite-jest": "^0.1.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@types/d3": "^7.1.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.19",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@vitejs/plugin-react": "^1.2.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest-environment-jsdom": "^27.5.1",
    "lint-staged": "^12.3.4",
    "prettier": "2.5.1",
    "typescript": "^4.5.4",
    "vite": "^2.8.0"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --cache --fix",
      "tsc-files --noEmit",
      "jest --bail --findRelatedTests"
    ],
    "src/**/*.{ts,tsx,css}": "prettier --write",
    "*.md": "prettier --write"
  }
}
