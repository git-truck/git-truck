{
  "env": {
      "browser": false,
      "es2021": true
  },
  "extends": [
      "@remix-run/eslint-config",
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react/jsx-runtime",
      "plugin:@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": [
      "@typescript-eslint",
      "react"
  ],
  "rules": {
      "no-case-declarations":"off",
      "react/jsx-no-useless-fragment": "warn"
  }
}

